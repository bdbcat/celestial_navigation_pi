#: src/Sight.cpp:508
msgid "Formulas used to calculate sight\n\n"
msgstr "Fòrmules utilitzades per calcular l'observació\n\n"

#: src/Sight.cpp:512
#, c-format
msgid "Index Error is %.4f degrees\n\n"
msgstr "Error de l'índex és %.4f graus\n\n"

#: src/Sight.cpp:517
#, c-format
msgid "Eye Height is %.4f meters\n"
"Height Correction Degrees = 1.758*sqrt(%.4f) / 60.0\n"
"Height Correction Degrees = %.4f\n"
msgstr "Alçada de l'ull és %.4f metres\n"
"Graus correcció alçada = 1.758*sqrt(%.4f) / 60.0\n"
"Graus correcció alçada = %.4f\n"

#: src/Sight.cpp:524
#, c-format
msgid "\n"
"Apparent Altitude (Ha)\n"
"ApparentAltitude = Measurement - IndexCorrection - EyeHeightCorrection\n"
"ApparentAltitude = %.4f - %.4f - %.4f\n"
"ApparentAltitude = %.4f\n"
msgstr "\n"
"Altitud aparent (hectàrees) \n"
"AltitudAparent = mesura - Correcció d'índex - Correcció alçada d'ull\n"
"AltitudAparent = %.4f - %.4f - %.4f\n"
"AltitudAparent = %.4f\n"

#: src/Sight.cpp:542
#, c-format
msgid "\n"
"Refraction Correction\n"
"x = tan(Pi/180*ApparentAltitude + 4.848e-2*(Pi/180) / (tan(Pi/180*ApparentAltitude) + .028))\n"
"x = tan(Pi/180*%.4f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.4f) + .028))\n"
"x = %.4f\n"
msgstr "\n"
"Correcció per Refracció\n"
"x = tan(Pi/180*Altura Aparent + 4.848e-2*(Pi/180) / (tan(Pi/180*Altura Aparent) + .028))\n"
"x = tan(Pi/180*%.4f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.4f) + .028))\n"
"x = %.4f\n"

#: src/Sight.cpp:547
#, c-format
msgid "RefractionCorrection = .267 * Pressure / (x*(Temperature + 273.15)) / 60.0\n"
"RefractionCorrection = .267 * %.4f / (x*(%.4f + 273.15)) / 60.0\n"
"RefractionCorrection = %.4f\n"
msgstr "Correcció per Refracció = .267 * Pressió / (x*(Temperatura + 273.15)) / 60.0\n"
"Correcció per Refracció = .267 * %.4f / (x*(%.4f + 273.15)) / 60.0\n"
"Correcció per Refracció = %.4f\n"

#: src/Sight.cpp:562
#, c-format
msgid "\n"
"Sun selected, Limb Correction\n"
"ra = %.4f, lc = 0.266564/ra = %.4f\n"
msgstr "\n"
"Sol sel·leccionat, Correcció per Limb\n"
"ra = %.4f, lc = 0.266564/ra = %.4f\n"

#: src/Sight.cpp:572
#, c-format
msgid "\n"
"Moon selected, Limb Correction\n"
"SD = %.4f\n"
"lc = 180/Pi * asin(Pi/180*SD)\n"
"lc = %.4f\n"
msgstr "\n"
"Lluna seleccionada, Correcció per Limb\n"
"SD = %.4f\n"
"lc = 180/Pi * asin(Pi/180*SD)\n"
"lc = %.4f\n"

#: src/Sight.cpp:582
msgid "Upper Limb"
msgstr "Limb superior"

#: src/Sight.cpp:585
msgid "Lower Limb"
msgstr "Limb inferior"

#: src/Sight.cpp:588
#, c-format
msgid "\n"
"LimbCorrection = %.4f\n"
msgstr "\n"
"Correcció per Limb = %.4f\n"

#: src/Sight.cpp:593
#, c-format
msgid "\n"
"Corrected Altitude\n"
"CorrectedAltitude = ApparentAltitude - RefractionCorrection - LimbCorrection\n"
"CorrectedAltitude = %.4f - %.4f - %.4f\n"
"CorrectedAltitude = %.4f\n"
msgstr "\n"
"Altura Corregida\n"
"Altura Corregida =Altura Aparent - Correcció per Refracció - Correcció per Limb\n"
"Altura Corregida = %.4f - %.4f - %.4f\n"
"Altura Corregida = %.4f\n"

#: src/Sight.cpp:607
#, c-format
msgid "\n"
"Sun selected, parallax correction\n"
"rad = %.4f, HP = 0.002442/rad = %.4f\n"
msgstr "\n"
"Sol sel·leccionat, Correcció per paralatge\n"
"rad = %.4f, HP = 0.002442/rad = %.4f\n"

#: src/Sight.cpp:616
#, c-format
msgid "\n"
"Moon selected, parallax correction\n"
"HP = %.4f\n"
msgstr "\n"
"Lluna sel·leccionada, Correcció per paralatge\n"
"HP = %.4f\n"

#: src/Sight.cpp:622
#, c-format
msgid "ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * CorrectedAltitude))\n"
"ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * %.4f))\n"
"ParallaxCorrection = %.4f\n"
msgstr "Correcció per paralatge = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * Altura corregida))\n"
"Correcció per paralatge = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * %.4f))\n"
"Correcció per paralatge = %.4f\n"

#: src/Sight.cpp:629
#, c-format
msgid "\n"
"Observed Altitude (Ho)\n"
"ObservedAltitude = CorrectedAltitude - ParallaxCorrection\n"
"ObservedAltitude = %.4f - %.4f\n"
"ObservedAltitude = %.4f\n"
msgstr "\n"
"Altura Observada (Ho)\n"
"Altura Observada = Altura Corregida - Correcció per paralatge\n"
"Altura Observada = %.4f - %.4f\n"
"Altura Observada = %.4f\n"

#: src/Sight.cpp:656
msgid "Almanac Data For "
msgstr "Dades de l'almanac per a "

#: src/Sight.cpp:657
#, c-format
msgid "\n"
"Geographical Position (lat, lon) = %.4f %.4f\n"
"GHAAST = %.0f %.1f'\n"
"SHA = %.0f %.1f'\n"
"GHA = %.0f %.1f'\n"
"Dec = %c %.0f %.1f'\n"
"SD = %.1f'\n"
"HP = %.1f'\n\n"
msgstr "\n"
"Posició geogràfica (lat, lon) = %.4f %.4f\n"
"GHAAST = %.0f %.1f'\n"
"SHA = %.0f %.1f'\n"
"GHA = %.0f %.1f'\n"
"Dec = %c %.0f %.1f'\n"
"SD = %.1f'\n"
"HP = %.1f'\n\n"

